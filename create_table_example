create table inventory (id integer primary key, brand text, price integer, quantity integer, aisle integer, country text);

insert into inventory values (1, 'ABC', 20, 10, 1, 'USA');
insert into inventory values (2, 'test', 20, 1, 2, 'China');
insert into inventory values (3, 'something',10, 2, 3, 'China');
insert into inventory values (4, 'ok', 2, 44, 3, 'Italy');
insert into inventory values (5, 'google', 3, 22, 4, 'Mexico');
insert into inventory values (6, 'USA', 3, 44, 2, 'Japan');
insert into inventory values (7, 'sso', 4, 23, 4, 'China');
