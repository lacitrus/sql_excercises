insert into inventory values (10, 'time machine', 49, 33, 2, 'USA');
insert into inventory values (11, 'big tree', 33, 22, 9, 'UK');
insert into inventory values (12, 'oreily', 3, 222, 3, 'Japan');
insert into inventory values (13, 'goodyear', 4, 232, 3, 'Korea');
insert into inventory values (14, 'such a good thing', 4, 3, 2, 'USA');
insert into inventory values (15, 'Oh my ...', 3, 32, 3, 'USA');

select brand, price, country from inventory group by country order by price;
